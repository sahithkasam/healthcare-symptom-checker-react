{
  "name": "healthcare-symptom-checker-frontend",
  "version": 2,
  "public": true,
  "github": {
    "silent": true
  },
  "functions": {
    "app/api/**/*.js": {
      "includeFiles": "**"
    }
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        }
      ]
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://healthcare-symptom-checker-react.onrender.com/api"
  }
}